

















/*每个页面公共css */

@font-face {
  font-family: "iconfont"; /* Project id 4053180 */
  src: url(data:font/woff2;base64,) format('woff2'),
       url(data:font/woff;base64,) format('woff'),
       url(data:font/ttf;base64,) format('truetype');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-gouwuche:before {
  content: "\e73d";
}

.icon-zengjia:before {
  content: "\e8c0";
}

.icon-jian:before {
  content: "\e7cb";
}

.icon-shanchu:before {
  content: "\e74b";
}


view {
	padding: 0;
	margin: 0;
	box-sizing: border-box;
}

/* 提示语 */
.hint {
	padding: 30rpx 0;
	text-align: center;
	color: #999;
	font-size: 28rpx;
}
/* 菜品购物车弹出动画 */
.cart_shoppinglist_anim {
	-webkit-animation: fadeIn .4s .1s ease both;
	        animation: fadeIn .4s .1s ease both;
}
@-webkit-keyframes fadeIn {
	0% {
		opacity: 0;
		-webkit-transform: translateY(800rpx);
		        transform: translateY(800rpx);
	}
	
	100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0);
	}
}
@keyframes fadeIn {
	0% {
		opacity: 0;
		-webkit-transform: translateY(800rpx);
		        transform: translateY(800rpx);
	}
	
	100% {
		opacity: 1;
		-webkit-transform: translateY(0);
		        transform: translateY(0);
	}
}

